    "startLogin": "watchify js/login.js -t babelify -d -o public/javascripts/login.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "body-parser": "~1.13.2",
    "classname": "0.0.0",
    "cookie-parser": "~1.3.5",
    "crypto": "0.0.3",
    "debounce": "^1.0.0",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "jsonfile": "^2.2.3",
    "morgan": "~1.6.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.7",
    "react-markdown": "^2.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-saga": "^0.10.2",
